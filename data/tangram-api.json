{
    "keys" : [
        {
            "address": "^/$",
            "level": 0,
            "options": ["cameras","lights", "styles", "sources","layers"]
        },
        {
            "address": "^/cameras/(\\w|\\_|\\-)+$",
            "level": 2,
            "options": ["type","vanishing_point", "focal_length", "active"]
        },
        {
            "address": "^/lights/(\\w|\\_|\\-)+$",
            "level": 2,
            "options": ["type","diffuse", "ambient","specular","direction", "position", "origin", "radius", "attenuation", "exponent"]
        },
        {
            "address": "^/scene$",
            "level": 1,
            "options": ["background"]
        },
        {
            "address": "^/scene/background$",
            "level": 2,
            "options": ["color"]
        },
        {
            "address": "^/styles/(\\w|\\_|\\-)+$",
            "level": 2,
            "options": ["base", "mix", "blend", "lighting", "texcoords", "animated", "material", "shaders"]
        },
        {
            "address": "^/styles/(\\w|\\_|\\-)+/material$",
            "level": 3,
            "options": ["normal","emission","ambient","diffuse","specular","shininess"]
        },
        {
            "address": "^/styles/(\\w|\\_|\\-)+/material/(normal|emission|ambient|diffuse|specular)$",
            "level": 4,
            "options": ["texture","mapping","scale","amount"]
        },
        {
            "address": "^/styles/(\\w|\\_|\\-)+/shaders$",
            "level": 3,
            "options": ["blocks"]
        },
        {
            "address": "^/styles/(\\w|\\_|\\-)+/shaders/blocks$",
            "level": 4,
            "options": ["global","position","width","normal","color","filter"]
        },
        {
            "address": "^/sources/(\\w|\\_|\\-)+$",
            "level": 2,
            "options": ["type", "url"]
        },
        {
            "address": "^/layers/(\\w|\\_|\\-)+$",
            "level": 2,
            "options": ["data", "draw", "properties", "filter", "extruded"]
        },
        {
            "address": "^/layers/(\\w|\\_|\\-)+/data$",
            "level": 3,
            "options": ["source"]
        },
        {
            "address": "^/layers/(\\w|\\_|\\-)+/draw$",
            "level": 3,
            "options": ["points", "lines", "polygons", "text"],
            "source": "/styles"
        },
        {
            "address": "^/layers/(\\w|\\_|\\-)+/draw/(\\w|\\_|\\-)+$",
            "level": 4,
            "options": ["style", "extrude", "color", "order", "width", "font"],
            "source": "/styles"
        },
        {
            "address": "^/layers/(\\w|\\_|\\-)+/draw/text/font$",
            "level": 5,
            "options": ["fill", "typeface"],
            "source": "/styles"
        },
        {
            "address": "^/layers/(\\w|\\_|\\-)+/properties$",
            "level": 3,
            "options": ["width"]
        }
    ],

    "values" : [

        {   "key": "visible", "default" : "true", "type" : "boolean" },
        {   "key": "join", "type" : "string", "options": ["miter","round", "bevel"] },
        {   "key": "cap", "type" : "string", "options": ["butt","round", "round"] },

        {   "key" : "^color$",    "type" : "color" },
        {   "key" : "^fill$",     "type" : "color" },
        {   "key": "^ambient$",   "type" : "color" },
        {   "key": "^diffuse$",   "type" : "color" },
        {   "key": "^specular$",  "type" : "color" },
        {   "key": "^emission$",  "type" : "color" },

        {
            "address": "/ambient/mapping$",
            "type" : "string",
            "options": ["uv","spheremap", "planar", "triplanar"]
        },
        {
            "address": "/diffuse/mapping$",
            "type" : "string",
            "options": ["uv","spheremap", "planar", "triplanar"]
        },
        {
            "address": "/specular/mapping$",
            "type" : "string",
            "options": ["uv","spheremap", "planar", "triplanar"]
        },
        {
            "address": "/emission/mapping$",
            "type" : "string",
            "options": ["uv","spheremap", "planar", "triplanar"]
        },
        {
            "address": "/normal/mapping$",
            "type" : "string",
            "options": ["uv", "planar", "triplanar" ]
        },
        {
            "address": "^/lights/(\\w|\\_|\\-)+/type$",
            "type" : "string",
            "options": ["ambient","directional", "point", "spotlight"]
        },
        {
            "address": "^/lights/(\\w|\\_|\\-)+/origin$",
            "type" : "string",
            "options": ["camera", "world","ground"]
        },

        {
            "address": "^/cameras/(\\w|\\_|\\-)+/type$",
            "type" : "string",
            "options": ["flat","perspective", "isometric"]
        },

        {
            "address": "^/sources/(\\w|\\_|\\-)+/type$",
            "type" : "string",
            "options": ["GeoJSON","TopoJSON", "MVT"]
        },

        {
            "address": "^/styles/(\\w|\\_|\\-)+/base",
            "type" : "string",
            "options": ["points", "lines", "polygons", "text"]
        },
        {
            "address": "^/styles/(\\w|\\_|\\-)+/blend$",
            "type" : "string",
            "options": ["add","multiply", "overlay"]
        },
        {
            "address": "^/styles/(\\w|\\_|\\-)+/mix$",
            "type" : "string",
            "source": "/styles"
        },
        {   
            "address": "^/styles/(\\w|\\_|\\-)+/texcoords$",
            "default" : "true",
            "type" : "boolean" 
        },
        {   
            "address": "^/styles/(\\w|\\_|\\-)+/lighting$",
            "default" : "true",
            "type" : "boolean" 
        },
        {   
            "address": "^/styles/(\\w|\\_|\\-)+/animated$",
            "default" : "true",
            "type" : "boolean" 
        },

        {   
            "address": "^/layers/(\\w|\\_|\\-|\\/)+/extrude$",
            "default" : "true",
            "type" : "boolean" 
        },
        { 
            "address": "^/layers/(\\w|\\_|\\-|\\/)+/tile_edges$",
            "default" : "true",
            "type" : "boolean" 
        },
        {
            "address": "^/layers/(\\w+|\\_|\\-|\\/)/draw/(\\w|\\_|\\-)+/style$",
            "type" : "string",
            "source": "/styles",
            "options": ["points", "lines", "polygons", "text"]
        },
        {
            "address": "^/layers/(\\w|\\_|\\-)+/data/source$",
            "type" : "string",
            "options": [""],
            "source": "/sources"
        }
    ]
}